{
  "compilerOptions": {
  "allowJs": true,
  "skipLibCheck": true,
  "strict": true,
  "forceConsistentCasingInFileNames": true,
  "esModuleInterop": true,
  "moduleResolution": "node",
  "resolveJsonModule": true,
  "jsx": "preserve",
  "incremental": true,
  "plugins": [
    {
      "name": "next"
    }
  ],
  "baseUrl": ".",
  "paths": {
    "@/*": ["./src/*"]
  },
  "module": "commonjs",
  "outDir": "dist",
  "lib": ["es2019"],
  "target": "es2019",
  "isolatedModules": false,
  "noEmit": false,
  "noUncheckedIndexedAccess": true,
  "composite": true
},
"include": [
  "env.d.ts",
  "index.ts",
  "src/**/*.ts"
],
"exclude": ["node_modules"]
}
